#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,temp;
    printf("Enter numbers seperated by space :\n");
    scanf("%d%d",&a,&b);
    printf("Before swaping a=%d and b=%d\n",a,b);
    temp=a;
    a=b;
    b=temp;
    printf("a=%d,b=%d",a,b);
    return 0;
}
